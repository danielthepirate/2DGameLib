        <EditorCategories value=""/>
        <On Terms="StatusOn.*.Jumping" Send="AnimBracketStart Fall Spell,D,Start Spell,D Spell,D,End"/>
        <On Terms="StatusOn.*.InAir; IsStatus Walking 1" Send="AnimClear Walk"/>
        <On Terms="StatusOff.*.InAir; IsStatus Walking 1" Send="AnimPlay Walk Walk PlayForever"/>
        <On Terms="StatusOn.*.Walking; !HasStatus InAir" Send="AnimPlay Walk Walk PlayForever"/>
        <On Terms="StatusOff.*.Walking; !HasStatus InAir" Send="AnimClear Walk"/>
        <On Terms="Behavior.UnitStateJumpingAnimation.On" Send="StatusSet Jumping 1"/>
        <On Terms="Behavior.UnitStateJumpingAnimation.Off" Send="StatusSet Jumping 0"/>
        <On Terms="Behavior.UnitStateWalkingAnimation.On" Send="StatusSet Walking 1"/>
        <On Terms="Behavior.UnitStateWalkingAnimation.Off" Send="StatusSet Walking 0"/>
        <On Terms="StatusOn.*.Jumping" Send="StatusIncrement InAir"/>
        <On Terms="StatusOff.*.Jumping" Send="StatusDecrement InAir"/>
        <On Terms="StatusOn.*.InAir; IsStatus Jumping 0" Send="TimerSet 0.187500 FallDelay"/>
        <On Terms="StatusOff.*.InAir" Send="AnimBracketStop Fall 0 0.180000 AsDuration"/>
        <On Terms="Behavior.UnitStateFallingAnimation.On" Send="StatusIncrement InAir"/>
        <On Terms="Behavior.UnitStateFallingAnimation.Off" Send="StatusDecrement InAir"/>
        <On Terms="TimerExpired; IsStatus Jumping 0; HasStatus InAir; TimerName FallDelay" Send="AnimBracketStart Fall {} Spell,D Spell,D,End OpeningPlayForever,Instant"/>
        <On Terms="TimerExpired; TimerName TurnRight; HasStatus TurningRight" Send="EventMacroRun TurnRightStopRunnable"/>
        <On Terms="TimerExpired; TimerName TurnLeft; HasStatus TurningLeft" Send="EventMacroRun TurnLeftStopRunnable"/>
        <On Terms="Behavior.UnitStateDying.On" Send="StatusSet Dying 1"/>
